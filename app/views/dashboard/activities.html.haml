.well
  %form
    .col-lg-2
      .form-group
        = select_month @filter_date, {add_month_numbers: false}, {name: 'month', :class => 'form-control'}
    .col-lg-2
      .form-group
        = select_year @filter_date, {add_month_numbers: false}, {name: 'year', :class => ' form-control'}
    .col-lg-2
      .form-group
        %a.btn.btn-info{:id => 'dashboard-activities'} Applica

  %table.table
    %tr
      %th Commessa
      - @users.each do |u|
        %td.right
          = u.name
      - @result.each do |a|
        %tr
          %th
            = a["code"]
          - @users.each do |u|
            %td.right
              = a[u.name]
        
        
